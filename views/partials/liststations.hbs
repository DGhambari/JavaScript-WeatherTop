{{#each stations}}
  <section class="ui stacked segment">
    <h2 class="ui header">
      {{name}}
    </h2>
    <p> Latitude: {{lat}} </p>
    <p> Longitude: {{lng}} </p>
    <a href="/station/{{id}}" class="ui icon button">
      <i class="icon folder open"></i>
    </a>
    <a href="/dashboard/deleteReading/{{id}}" class="ui icon button">
      <i class="icon trash"></i>
    </a>
  </section>
{{/each}}
